<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context=".MainActivity$PlaceholderFragment">

    <TextView
            android:id="@+id/section_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Favorite"
            android:id="@+id/button" android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" android:clickable="true" android:onClick="myFavorite"
            android:layout_alignRight="@+id/button2" android:layout_alignEnd="@+id/button2"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="I Broke My Back"
            android:id="@+id/button2" android:layout_below="@+id/button" android:layout_toRightOf="@+id/section_label"
            android:layout_toEndOf="@+id/section_label" android:onClick="iBrokeMyBack"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back Is Broken"
            android:id="@+id/button3" android:layout_below="@+id/button2" android:layout_toRightOf="@+id/section_label"
            android:layout_alignRight="@+id/button2" android:layout_alignEnd="@+id/button2"
            android:onClick="backIsBroken"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Spinal"
            android:id="@+id/button4"
            android:layout_below="@+id/button3" android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" android:layout_alignRight="@+id/button3"
            android:layout_alignEnd="@+id/button3" android:onClick="spinal"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AARON"
            android:id="@+id/button5"
            android:layout_above="@+id/button2" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:onClick="aaron" android:layout_alignLeft="@+id/button7"
            android:layout_alignStart="@+id/button7"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Crystal Light"
            android:id="@+id/button6" android:layout_alignTop="@+id/button2" android:layout_alignRight="@+id/button5"
            android:layout_alignEnd="@+id/button5" android:onClick="crystalLight"
            android:layout_alignLeft="@+id/button7" android:layout_alignStart="@+id/button7"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Smart Enough"
            android:id="@+id/button7" android:layout_alignBottom="@+id/button3"
            android:layout_alignTop="@+id/button3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:onClick="smartEnough"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Murder"
            android:id="@+id/button8" android:layout_alignBottom="@+id/button4" android:layout_alignLeft="@+id/button7"
            android:layout_alignStart="@+id/button7" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:onClick="murder"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Smart Man"
            android:id="@+id/button9" android:layout_below="@+id/button4" android:layout_alignRight="@+id/button4"
            android:layout_alignEnd="@+id/button4" android:layout_toRightOf="@+id/section_label"
            android:layout_toEndOf="@+id/section_label" android:onClick="imNotASmartMan"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="What Love Is"
            android:id="@+id/button10" android:layout_alignBottom="@+id/button9" android:layout_alignLeft="@+id/button8"
            android:layout_alignStart="@+id/button8" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:onClick="butIKnowWhatLoveIs"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="YEAH"
            android:id="@+id/button11" android:layout_below="@+id/button9" android:layout_alignRight="@+id/button9"
            android:layout_alignEnd="@+id/button9" android:layout_toRightOf="@+id/section_label"
            android:layout_toEndOf="@+id/section_label" android:onClick="yeah" android:singleLine="true"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Goddamn Right"
            android:id="@+id/button12" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:layout_alignLeft="@+id/button10"
            android:layout_alignStart="@+id/button10" android:onClick="youreGoddamnedRight"
            android:layout_below="@+id/button10" android:layout_alignBottom="@+id/button11" android:textSize="12dp"/>

</RelativeLayout>
